<resources xmlns:xliff="urn:oasis:names:tc:xliff:document:1.2">

    <!-- Defines the location of the fingerprint sensor on the device
         0 = back
         1 = front
         2 = left side
         3 = right side
    -->
    <integer name="config_fingerprintSensorLocation">0</integer>

    <!-- Whether to show min/max refresh rate in display settings -->
    <bool name="config_show_refresh_rate_controls">false</bool>

    <!-- Whether to show Google account avatar in settings -->
    <bool name="config_show_google_account_avatar">false</bool>

    <!-- Battery health -->
    <string name="config_batteryHealthNode"></string>
    <string name="config_batteryRemainingCapacityNode"></string>
    <string name="config_batteryDesignCapacityNode"></string>

</resources>
